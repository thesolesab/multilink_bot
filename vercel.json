{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "api/webhook.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    }
}